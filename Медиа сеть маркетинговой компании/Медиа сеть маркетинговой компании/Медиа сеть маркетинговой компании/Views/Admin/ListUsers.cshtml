@model List<Медиа_сеть_маркетинговой_компании.Models.User>
@{
    Layout = "~/Views/Shared/_LayoutRooms.cshtml";
    ViewBag.Section_Title = "Пользователи";
    ViewBag.Title = "Административная панель";
}
        <article class="module width_3_quarter">
            <header>
                <h3 class="tabs_involved">Списсок пользователей</h3>
                <ul class="tabs">
                    <li><a href="#tab1">Пользователи</a></li>
                </ul>
            </header>

            <div class="tab_container">
                <div id="tab1" class="tab_content">
                    <table class="tablesorter" cellspacing="0">
                        <thead>
                            <tr>
                                <th>№</th>
                                @*<th>Имя</th>
                                <th>Фамилия</th>*@
                                <th>Логин</th>
                                @*<th>Роль</th>
                                <th>Возраст</th>*@
                                <th>Действия</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (Медиа_сеть_маркетинговой_компании.Models.User m in Model)
                            {
                                <tr>
                                    <td>@m.Id</td>
                                    @*<td>@m.Profile.FirstName</td>
                                    <td>@m.Profile.LastName</td>*@
                                    <td>@m.Email</td>
                                    @*<td>@m.Role.Name</td>
                                    <td>@m.Profile.Age</td>*@
                                <td><a href="../Admin/EditUser/@m.Id.ToString()"><input type="image" src="~/Content/mediaadmin/images/icn_edit.png" title="Edit"></a><a href="../Admin/DelUser/@m.Id.ToString()"><input type="image" src="~/Content/mediaadmin/images/icn_trash.png" title="Trash"></a></td>
                            </tr>
                            }
                        </tbody>
                    </table>
                </div><!-- end of #tab1 -->
            </div><!-- end of .tab_container -->

        </article><!-- end of content manager article -->